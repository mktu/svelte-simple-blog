{
	"name": "svelte-blog",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "rm -rf build && vite build",
		"build:test": "rm -rf build && NODE_ENV=test vite build --mode test",
		"deploy": "rm -rf node_modules/.cache/gh-pages && npm run build && gh-pages -d build -t true",
		"package": "svelte-kit package",
		"preview": "NODE_ENV=test vite preview",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/test": "1.33.0",
		"@sveltejs/adapter-static": "2.0.2",
		"@sveltejs/kit": "1.20.5",
		"@typescript-eslint/eslint-plugin": "5.60.1",
		"@typescript-eslint/parser": "5.60.1",
		"eslint": "8.42.0",
		"eslint-config-prettier": "8.8.0",
		"eslint-plugin-svelte3": "4.0.0",
		"gh-pages": "5.0.0",
		"prettier": "2.8.8",
		"prettier-plugin-svelte": "2.10.1",
		"sass": "1.63.6",
		"svelte": "^4.0.0",
		"svelte-check": "3.4.6",
		"svelte-preprocess": "5.0.4",
		"tslib": "2.5.3",
		"typescript": "5.1.6",
		"vite": "4.3.9"
	},
	"type": "module",
	"dependencies": {
		"date-fns": "2.30.0",
		"newt-client-js": "3.2.5",
		"svelte-intersection-observer": "0.10.0",
		"svelte-markdown": "0.2.3",
		"svelte-meta-tags": "2.8.0",
		"svelte-simple-modal": "1.5.2"
	}
}