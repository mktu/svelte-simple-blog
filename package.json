{
	"name": "svelte-blog",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "rm -rf build && vite build",
		"build:test": "rm -rf build && NODE_ENV=test vite build --mode test",
		"deploy": "rm -rf node_modules/.cache/gh-pages && npm run build && gh-pages -d build -t true",
		"package": "svelte-kit package",
		"preview": "NODE_ENV=test vite preview",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@playwright/test": "1.28.1",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "1.0.0",
		"@sveltejs/kit": "1.0.1",
		"@typescript-eslint/eslint-plugin": "5.46.1",
		"@typescript-eslint/parser": "5.46.1",
		"eslint": "8.30.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "4.0.0",
		"gh-pages": "4.0.0",
		"prettier": "2.8.1",
		"prettier-plugin-svelte": "2.9.0",
		"sass": "1.56.2",
		"svelte": "3.55.0",
		"svelte-check": "2.10.2",
		"svelte-preprocess": "5.0.0",
		"tslib": "2.4.1",
		"typescript": "4.9.4",
		"vite": "4.0.3"
	},
	"type": "module",
	"dependencies": {
		"date-fns": "2.29.3",
		"newt-client-js": "3.2.2",
		"svelte-intersection-observer": "0.10.0",
		"svelte-markdown": "0.2.3",
		"svelte-meta-tags": "2.6.4",
		"svelte-simple-modal": "1.4.5"
	}
}
