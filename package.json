{
	"name": "svelte-blog",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "rm -rf build && svelte-kit build",
		"deploy": "rm -rf node_modules/.cache/gh-pages && npm run build && gh-pages -d build -t true",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@fontsource/m-plus-1": "4.5.9",
		"@playwright/test": "1.22.2",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "1.0.0-next.34",
		"@sveltejs/kit": "next",
		"@typescript-eslint/eslint-plugin": "5.15.0",
		"@typescript-eslint/parser": "5.15.0",
		"eslint": "7.32.0",
		"eslint-config-prettier": "8.5.0",
		"eslint-plugin-svelte3": "3.4.1",
		"gh-pages": "3.2.3",
		"prettier": "2.6.2",
		"prettier-plugin-svelte": "2.6.0",
		"sass": "1.49.11",
		"svelte": "3.46.6",
		"svelte-check": "2.4.6",
		"svelte-preprocess": "4.10.6",
		"tslib": "2.3.1",
		"typescript": "4.6.4"
	},
	"type": "module",
	"dependencies": {
		"date-fns": "2.28.0",
		"newt-client-js": "2.0.2",
		"svelte-intersection-observer": "0.10.0",
		"svelte-markdown": "0.2.2",
		"svelte-meta-tags": "2.5.5",
		"svelte-simple-modal": "1.3.4"
	}
}
